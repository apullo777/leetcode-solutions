# https://leetcode.com/problems/two-sum/

# Time: O(n)

class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        dictionary = {}
        for idx, num in enumerate(nums):
            complement = target - num
            if complement in dictionary:
                return dictionary[complement], idx
            else:
                dictionary[num] = idx